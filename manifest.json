{
  "name": "print-toConsole-andFile",
  "description": "Adds a print button to the browser.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
      "default_title": "print-toConsole-andFile",
      "default_icon": {
        "16": "image/print_16x16.png",
        "32": "image/print_32x32.png",
        "48": "image/print_48x48.png",
        "128":"image/print_128x128.png"
      }
  },
  "icons": {
    "16": "image/print_16x16.png",
    "32": "image/print_32x32.png",
    "48": "image/print_48x48.png",
    "128":"image/print_128x128.png"
  },
  "manifest_version": 2
}

